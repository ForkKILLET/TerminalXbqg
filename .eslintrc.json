{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": [ 2, { "code": 125 } ],
        "no-empty": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-useless-escape": 0,
		"no-debugger": 0,
		"no-control-regex": 0,
		"no-constant-condition": 0
    }
}
